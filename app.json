{
  "expo": {
    "name": "Pocket Chef",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png"
    },
    "version": "1.1.0",
    "slug": "PocketChef",
    "description": "All your recipes in your pocket.",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "fr.ghelouis.pocketchef",
      "versionCode": 2,
      "permissions": ["CAMERA", " READ_EXTERNAL_STORAGE"]
    },
    "ios": {
      "bundleIdentifier": "fr.ghelouis.pocketchef",
      "buildNumber": "1.1.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "For taking a photo of your recipe",
        "NSPhotoLibraryUsageDescription": "For importing a photo on a recipe"
      }
    },
    "extra": {
      "developerEmail": "pocketchef@ghelouis.fr",
      "license": "GPLv3",
      "sourceCodeUrl": "https://github.com/ghelouis/pocketchef"
    }
  }
}
